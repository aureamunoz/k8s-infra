apiVersion: v1
kind: Service
metadata:
  name: {{ registry.service_name}}
  namespace: {{ registry.namespace }}
  labels:
    k8s-app: {{ registry.service_name}}
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "KubeRegistry"
spec:
  type: ClusterIP
  selector:
    k8s-app: {{ registry.service_name}}
  ports:
    - name: registry
      port: 5000
      protocol: TCP
